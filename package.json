{
    "name": "LookupProtocol",
    "version": "2.0.0",
    "description": "Modern product lookup system with REST API and database",
    "keywords": ["product", "lookup", "api", "database", "rest"],
    "author": "joshu",
    "contributors": [],
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "init-db": "node scripts/init-db.js",
        "seed": "node scripts/seed-db.js",
        "setup": "npm run init-db && npm run seed",
        "test": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "sqlite3": "^5.1.6",
        "body-parser": "^1.20.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "winston": "^3.11.0",
        "swagger-ui-express": "^5.0.0",
        "swagger-jsdoc": "^6.2.8"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "jest": "^29.7.0",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ]
    }
}
